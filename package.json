{
  "name": "simple-countdown-for-mac",
  "productName": "Countdown",
  "version": "1.0.0",
  "description": "An Electron application to create countdown timers",
  "main": "main.js",
  "scripts": {
    "start": "webpack --display-error-details & ./node_modules/.bin/electron .",
    "build": "webpack -w --progress --display-error-details",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint **/*.js",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icon.icns --prune=true --out=release-builds"
  },
  "keywords": [
    "Electron",
    "countdown"
  ],
  "author": "Megas Liu",
  "license": "MIT",
  "dependencies": {
    "electron": "^1.6.11",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "styled-components": "^2.1.0"
  },
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-core": "6.25.0",
    "babel-eslint": "7.2.3",
    "babel-loader": "^7.1.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "electron-packager": "8.7.1",
    "eslint": "4.0.0",
    "eslint-plugin-prettier": "2.1.2",
    "eslint-plugin-react": "7.1.0",
    "prettier": "1.4.4",
    "webpack": "^3.0.0"
  }
}